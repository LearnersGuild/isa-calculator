{
  "name": "isa-calculator",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 9010",
    "build": "next build",
    "start": "npm run toolbox && next start",
    "test": "NODE_ENV=test ./node_modules/.bin/tape -r babel-register '!(node_modules)/__tests__/*.test.js' | ./node_modules/.bin/tap-spec",
    "toolbox": "react-toolbox-themr"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.18.1",
    "next": "3.0.1-beta.13",
    "next-redux-wrapper": "^1.0.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "react-toolbox-themr": "^1.0.2",
    "tape": "^4.7.0"
  },
  "reactToolbox": {
    "include": [
      "APP_BAR",
      "BUTTON",
      "CARD",
      "DATE_PICKER",
      "INPUT",
      "LINK",
      "LIST",
      "SWITCH"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "appbar-color": "var(--color-slate-grey)",
      "appbar-contrast": "var(--color-light-grey)",
      "color-blue": "#00A3DF",
      "color-green": "#97C93D",
      "color-red": "#ED3F60",
      "color-yellow": "#F2BB1A",
      "color-dark-blue": "#4A539F",
      "color-dark-green": "#1B8C5C",
      "color-dark-red": "#981C4D",
      "color-orange": "#E47226",
      "color-slate-grey": "#252531",
      "color-light-grey": "#E2E3E4",
      "color-divider": "var(--color-light-grey)",
      "color-text": "var(--color-slate-grey)",
      "color-primary": "var(--color-blue)",
      "color-primary-dark": "var(--color-dark-blue)",
      "color-accent": "var(--color-red)",
      "color-accent-dark": "var(--color-dark-red)",
      "input-text-error-color": "var(--color-red)"
    },
    "output": "static"
  }
}
